set terminal pdfcairo
set output 'plot.pdf'

set datafile separator ','

d=0

f(x) = A*exp(-((x-u)/r)**2/2)

A=400
u=200
r=100

fit f(x) 'chuchito.csv' using 0:1 every :::0::d via A,u,r

set xrange [0:20]
# plot 'chuchito.csv' using 0:1, f(x)
plot 'chuchito.csv' using 0:1, 'data.csv' every :::0::d using 0:1, f(x)